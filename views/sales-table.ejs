<%- include('parts/html-head') %>
<%- include('parts/navbar') %>
<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">員工編號</th>
                <th scope="col">姓名</th>
                <th scope="col">年齡</th>
            </tr>
        </thead>
        <tbody>
            <% for(let i in test) { %>
                <tr>
                    <td>
                        <%= test[i].id %>
                    </td>
                    <td>
                        <%= test[i].name %>
                    </td>
                    <td>
                        <%= test[i].age %>
                    </td>
                </tr>
                <% } %>
        </tbody>
    </table>
</div>
<%- include('parts/html-foot') %>